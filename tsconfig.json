{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "~/assets/*": ["src/assets/*"],
      "~/components/*": ["src/components/*"],
      "~/layouts/*": ["src/layouts/*"],
      "~/utils/*": ["src/utils/*"]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  }
}
