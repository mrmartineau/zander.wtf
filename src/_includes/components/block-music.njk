<div class="zm-block zm-music">
  <header>
    <h3 class="zm-block-title">Listening</h3>
  </header>
  <div class="zm-music-list">
    {% for item in collections.music.topArtists %} {% set image =
    item.image[3]['#text'] %} {% if image %}
    <img src="{{ image }}" alt="{{ item.name }}" class="zm-music-item" /> {%
    endif %} {% endfor %}
  </div>
</div>
